input random
numero_secret = random.randint(1,10)
tentativa = 0
max_tentativas = 5
print("bem vindo ao jogo")
print("tente adivinhar um simples numero entre 1 e 10")
while tentativa < max_tentativas:
    palpite = int(input("digite um numero entre 1 e 10"))
    tentativa +=1 
    if palpite == numero_secret:
        print(f"parabens voce adivinhou o numero {numero_secret}")
        break
    elif palpite < numero_secret